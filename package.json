{
  "name": "clickstream-js",
  "version": "0.1.0",
  "private": true,
  "description": "ClickstreamAnalytics Web SDK",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run clean && npx tsc && npx webpack --config webpack.config.js",
    "build-dev": "npm run clean && npx tsc && npx webpack --config webpack.config.dev.js",
    "format": "npx prettier --check 'src/**/*.{js,ts}'",
    "lint": "npx eslint src",
    "test": "npx jest -w 1 --coverage",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@aws-amplify/core": "^5.5.1",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.0",
    "@aws-crypto/sha256-browser": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.1.0",
    "typescript": "^4.9.5",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "@types/jsdom": "^21.1.1",
    "jest-environment-jsdom": "29.5.0",
    "fetch-mock": "^9.11.0"
  },
  "engines": {
    "node": ">=16.19.0"
  }
}
